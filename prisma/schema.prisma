generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Account {
  id                String  @id @default(cuid())
  userId            String
  type              String
  provider          String
  providerAccountId String
  refresh_token     String? @db.Text
  access_token      String? @db.Text
  expires_at        Int?
  token_type        String?
  scope             String?
  id_token          String? @db.Text
  session_state     String?
  user              User    @relation(fields: [userId], references: [id], onDelete: Cascade, map: "Account_userId_fkey")

  @@unique([provider, providerAccountId], map: "Account_provider_providerAccountId_key")
  @@index([userId], map: "Account_userId_fkey")
}

model AccountMember {
  id                                      String   @id @default(cuid())
  accountOwnerId                          String
  memberUserId                            String
  role                                    String   @default("member")
  createdAt                               DateTime @default(now())
  user_accountmember_accountOwnerIdTouser User     @relation("accountmember_accountOwnerIdTouser", fields: [accountOwnerId], references: [id], onDelete: Cascade, map: "AccountMember_accountOwnerId_fkey")
  user_accountmember_memberUserIdTouser   User     @relation("accountmember_memberUserIdTouser", fields: [memberUserId], references: [id], onDelete: Cascade, map: "AccountMember_memberUserId_fkey")

  @@unique([accountOwnerId, memberUserId], map: "AccountMember_accountOwnerId_memberUserId_key")
  @@index([accountOwnerId], map: "AccountMember_accountOwnerId_idx")
  @@index([memberUserId], map: "AccountMember_memberUserId_idx")
}

model ApiKey {
  id         String    @id @default(cuid())
  userId     String
  name       String    @db.VarChar(255)
  keyPrefix  String    @db.VarChar(20)
  keyHash    String    @db.VarChar(64)
  lastUsedAt DateTime?
  createdAt  DateTime  @default(now())
  user       User      @relation(fields: [userId], references: [id], onDelete: Cascade, map: "ApiKey_userId_fkey")

  @@index([keyHash], map: "ApiKey_keyHash_idx")
  @@index([userId], map: "ApiKey_userId_idx")
}

model Bot {
  id                   String     @id @default(cuid())
  name                 String
  userId               String
  greetingMessage      String     @default("Hi! How can I help you today?") @db.VarChar(500)
  tone                 String     @default("friendly")
  confidenceThreshold  Float      @default(0.7)
  leadCaptureTrigger   String     @default("uncertain")
  humanFallbackMessage String     @default("I'm not sure about that. Would you like to leave your contact and we'll get back to you?") @db.VarChar(500)
  publicKey            String?    @unique(map: "Bot_publicKey_key")
  createdAt            DateTime   @default(now())
  updatedAt            DateTime
  quickPrompts         String?    @db.Text
  removeBranding       Boolean    @default(false)
  user                 User       @relation(fields: [userId], references: [id], onDelete: Cascade, map: "Bot_userId_fkey")
  chat                 Chat[]
  chunk                Chunk[]
  lead                 Lead[]
  source               Source[]
  usagelog             UsageLog[]

  @@index([publicKey], map: "Bot_publicKey_idx")
  @@index([userId], map: "Bot_userId_idx")
}

model Chat {
  id           String        @id @default(cuid())
  botId        String
  visitorId    String?
  visitorEmail String?
  visitorName  String?
  pageUrl      String?       @db.Text
  metadata     String?       @db.LongText
  createdAt    DateTime      @default(now())
  updatedAt    DateTime
  bot          Bot           @relation(fields: [botId], references: [id], onDelete: Cascade, map: "Chat_botId_fkey")
  chatmessage  ChatMessage[]

  @@index([botId], map: "Chat_botId_idx")
  @@index([createdAt], map: "Chat_createdAt_idx")
  @@index([visitorId], map: "Chat_visitorId_idx")
}

model ChatMessage {
  id        String   @id @default(cuid())
  chatId    String
  role      String
  content   String   @db.LongText
  sources   String?  @db.LongText
  createdAt DateTime @default(now())
  chat      Chat     @relation(fields: [chatId], references: [id], onDelete: Cascade, map: "ChatMessage_chatId_fkey")

  @@index([chatId], map: "ChatMessage_chatId_idx")
}

model Chunk {
  id         String     @id @default(cuid())
  botId      String
  sourceId   String
  content    String     @db.LongText
  metadata   String?    @db.LongText
  tokenCount Int        @default(0)
  createdAt  DateTime   @default(now())
  bot        Bot        @relation(fields: [botId], references: [id], onDelete: Cascade, map: "Chunk_botId_fkey")
  source     Source     @relation(fields: [sourceId], references: [id], onDelete: Cascade, map: "Chunk_sourceId_fkey")
  embedding  Embedding?

  @@index([botId], map: "Chunk_botId_idx")
  @@index([sourceId], map: "Chunk_sourceId_idx")
}

model ContactSubmission {
  id        String   @id @default(cuid())
  name      String   @db.VarChar(255)
  email     String   @db.VarChar(255)
  subject   String   @db.VarChar(500)
  message   String   @db.Text
  status    String   @default("new")
  createdAt DateTime @default(now())

  @@index([createdAt], map: "ContactSubmission_createdAt_idx")
  @@index([status], map: "ContactSubmission_status_idx")
}

model Embedding {
  id        String   @id @default(cuid())
  chunkId   String   @unique(map: "Embedding_chunkId_key")
  botId     String
  vector    String   @db.LongText
  createdAt DateTime @default(now())
  chunk     Chunk    @relation(fields: [chunkId], references: [id], onDelete: Cascade, map: "Embedding_chunkId_fkey")

  @@index([botId], map: "Embedding_botId_idx")
}

model Lead {
  id        String   @id @default(cuid())
  botId     String
  name      String?
  email     String
  message   String?  @db.Text
  pageUrl   String?  @db.Text
  metadata  String?  @db.LongText
  createdAt DateTime @default(now())
  phone     String?
  status    String   @default("new")
  bot       Bot      @relation(fields: [botId], references: [id], onDelete: Cascade, map: "Lead_botId_fkey")

  @@index([botId], map: "Lead_botId_idx")
  @@index([email], map: "Lead_email_idx")
  @@index([status], map: "Lead_status_idx")
}

model Plan {
  id                    String     @id @default(cuid())
  name                  String
  dailyLimit            Int        @default(100)
  botLimit              Int        @default(3)
  storageLimit          Int        @default(50)
  price                 Float      @default(0)
  isActive              Boolean    @default(true)
  createdAt             DateTime   @default(now())
  updatedAt             DateTime
  teamMemberLimit       Int        @default(1)
  paypalPlanIdMonthly   String?
  paypalPlanIdYearly    String?
  razorpayPlanIdMonthly String?
  razorpayPlanIdYearly  String?
  stripePriceIdMonthly  String?
  stripePriceIdYearly   String?
  userplan              UserPlan[]

  @@index([name], map: "Plan_name_idx")
}

model Session {
  id           String   @id @default(cuid())
  sessionToken String   @unique(map: "Session_sessionToken_key")
  userId       String
  expires      DateTime
  user         User     @relation(fields: [userId], references: [id], onDelete: Cascade, map: "Session_userId_fkey")

  @@index([userId], map: "Session_userId_fkey")
}

model Source {
  id              String    @id @default(cuid())
  botId           String
  type            String
  url             String?   @db.Text
  documentUrl     String?   @db.Text
  title           String?
  status          String    @default("pending")
  pageCount       Int       @default(0)
  error           String?   @db.Text
  createdAt       DateTime  @default(now())
  updatedAt       DateTime
  lastRefreshedAt DateTime?
  chunk           Chunk[]
  bot             Bot       @relation(fields: [botId], references: [id], onDelete: Cascade, map: "Source_botId_fkey")

  @@index([botId], map: "Source_botId_idx")
  @@index([status], map: "Source_status_idx")
}

model TeamInvitation {
  id             String   @id @default(cuid())
  accountOwnerId String
  email          String
  role           String   @default("member")
  token          String   @unique(map: "TeamInvitation_token_key")
  expiresAt      DateTime
  createdAt      DateTime @default(now())
  user           User     @relation(fields: [accountOwnerId], references: [id], onDelete: Cascade, map: "TeamInvitation_accountOwnerId_fkey")

  @@index([accountOwnerId], map: "TeamInvitation_accountOwnerId_idx")
  @@index([token], map: "TeamInvitation_token_idx")
}

model UsageLog {
  id        String   @id @default(cuid())
  botId     String
  type      String
  count     Int      @default(1)
  metadata  String?  @db.LongText
  createdAt DateTime @default(now())
  bot       Bot      @relation(fields: [botId], references: [id], onDelete: Cascade, map: "UsageLog_botId_fkey")

  @@index([botId, createdAt], map: "UsageLog_botId_createdAt_idx")
  @@index([botId], map: "UsageLog_botId_idx")
  @@index([createdAt], map: "UsageLog_createdAt_idx")
}

model User {
  id                                               String           @id @default(cuid())
  email                                            String           @unique(map: "User_email_key")
  emailVerified                                    DateTime?
  name                                             String?
  image                                            String?
  password                                         String?
  createdAt                                        DateTime         @default(now())
  updatedAt                                        DateTime
  removeBrandingAddOn                              Boolean          @default(false)
  termsAcceptedAt                                  DateTime?
  stripeCustomerId                                 String?          @unique(map: "User_stripeCustomerId_key")
  account                                          Account[]
  accountmember_accountmember_accountOwnerIdTouser AccountMember[]  @relation("accountmember_accountOwnerIdTouser")
  accountmember_accountmember_memberUserIdTouser   AccountMember[]  @relation("accountmember_memberUserIdTouser")
  apikey                                           ApiKey[]
  bot                                              Bot[]
  session                                          Session[]
  teaminvitation                                   TeamInvitation[]
  userplan                                         UserPlan?
  webhook                                          Webhook[]
}

model UserPlan {
  id                     String    @id @default(cuid())
  userId                 String    @unique(map: "UserPlan_userId_key")
  planId                 String
  startsAt               DateTime  @default(now())
  endsAt                 DateTime?
  createdAt              DateTime  @default(now())
  currentPeriodEnd       DateTime?
  paypalSubscriptionId   String?
  razorpaySubscriptionId String?
  stripePriceId          String?
  stripeSubscriptionId   String?
  plan                   Plan      @relation(fields: [planId], references: [id], onDelete: Cascade, map: "UserPlan_planId_fkey")
  user                   User      @relation(fields: [userId], references: [id], onDelete: Cascade, map: "UserPlan_userId_fkey")

  @@index([planId], map: "UserPlan_planId_fkey")
  @@index([userId], map: "UserPlan_userId_idx")
}

model VerificationToken {
  id         String   @id @default(cuid())
  identifier String
  token      String   @unique(map: "VerificationToken_token_key")
  expires    DateTime

  @@unique([identifier, token], map: "VerificationToken_identifier_token_key")
}

model Webhook {
  id          String   @id @default(cuid())
  userId      String
  url         String   @db.Text
  secret      String   @db.VarChar(64)
  events      String   @db.VarChar(255)
  description String?  @db.VarChar(500)
  createdAt   DateTime @default(now())
  user        User     @relation(fields: [userId], references: [id], onDelete: Cascade, map: "Webhook_userId_fkey")

  @@index([userId], map: "Webhook_userId_idx")
}
